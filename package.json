{
  "name": "peeredit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "worker:types": "wrangler types  --cwd worker",
    "worker:check-types": "tsc --project worker/tsconfig.json",
    "worker:dev": "npm run worker:types && wrangler dev --cwd worker",
    "worker:deploy": "npm run worker:types && npm run worker:check-types && wrangler deploy --cwd worker",
    "worker:test": "vitest"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.5.1",
    "filepond-plugin-file-encode": "^2.1.11",
    "filepond-plugin-image-exif-orientation": "^1.0.11",
    "filepond-plugin-image-preview": "^4.6.11",
    "framer-motion": "^10.16.4",
    "lodash.debounce": "^4.0.8",
    "peerjs": "^1.5.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-filepond": "^7.1.2",
    "react-icons": "^4.11.0",
    "shepherd.js": "^12.0.4",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "vite": "^4.5.3",
    "@cloudflare/vitest-pool-workers": "^0.8.19",
    "typescript": "^5.5.2",
    "vitest": "~3.0.7",
    "wrangler": "^4.16.1"
  }
}